usicTop.vue"],"sourcesContent":["<template>\r\n    <div class=\"listViewTop\">\r\n        <img class=\"bg\" :src=\"playlist.coverImgUrl\" alt=\"\">\r\n        <div class=\"listViewTopNav\">\r\n            <div class=\"left\">\r\n                <!-- 返回按钮 -->\r\n                <div class=\"back\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\">\r\n                        <use xlink:href=\"#icon-xitongfanhui\"></use>\r\n                    </svg>\r\n                </div>\r\n                <!-- 歌单 -->\r\n                <div class=\"title\">\r\n                    歌单\r\n                </div>\r\n            </div>\r\n            <!-- 搜索按钮 和 更多按钮-->\r\n            <div class=\"right\">\r\n                <div class=\"search\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\">\r\n                        <use xlink:href=\"#icon-sousuo\"></use>\r\n                    </svg>\r\n                </div>\r\n                <div class=\"more\">\r\n                    <svg class=\"icon\" aria-hidden=\"true\">\r\n                        <use xlink:href=\"#icon-gengduo-shuxiang\"></use>\r\n                    </svg>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- 头像 简介部分 -->\r\n        <div class=\"content\">\r\n            <div class=\"content_left\">\r\n                <!-- 头像 -->\r\n                <div class=\"hp\">\r\n                    <img class=\"headPortrait\" :src=\"playlist.coverImgUrl\" alt=\"\">\r\n                     <div class=\"count\">\r\n                        <span>▷</span>\r\n                        <!--播放量  -->\r\n                        <span>{{formatNum(playlist.playCount)}}</span>\r\n                    </div>\r\n                </div>\r\n                <!-- 头像下面的小方块 -->\r\n                <div class=\"headPortraitBlock\">\r\n                    <img class=\"headPortraitBlockBG\" :src=\"playlist.coverImgUrl\" alt=\"\">\r\n                </div>\r\n            </div>\r\n            <div class=\"contnet_right\">\r\n                <div class=\"rightTop\">\r\n                <!-- 歌单用户姓名 -->\r\n                    <div class=\"UserTitle\">\r\n                        <p>{{playlist.name}}</p>\r\n                    </div>\r\n                <!-- 更多图标 -->\r\n                    <div class=\"contnet_right_more\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-gengduo\"></use>\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n                <!-- 用户头像和姓名 -->\r\n                <div class=\"UserInfo\">\r\n                    <!-- 头像 -->\r\n                    <div class=\"UserImg\">\r\n                        <img class=\"UserPortrait\"  :src=\"playlist.creator.avatarUrl\">\r\n                    </div>\r\n                    <!-- 名称 -->\r\n                    <div class=\"UserName\">\r\n                        <p>{{playlist.creator.nickname}}</p>\r\n                    </div>\r\n                    <!-- 关注图标 -->\r\n                    <div class=\"UserAttention\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-plus\"></use>\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n                <!-- 用户简介 -->\r\n                <div class=\"UserAbout\">\r\n                    <div class=\"description\">\r\n                        <p>{{playlist.description}}</p>\r\n                    </div>\r\n                    <div class=\"UserAboutMore\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-gengduo1\"></use>\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nexport default {\r\n    props:['playlist'],\r\n    setup(){\r\n     // 格式化输出听歌人数\r\n        const formatNum = computed(()=>{\r\n           return function(num){\r\n                if(num>10000) return (num/10000).toFixed(2) + '万'\r\n            }\r\n        })\r\n        return {\r\n            formatNum\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.listViewTop{\r\n    width: 7.5rem;\r\n    padding: 0 0.4rem;\r\n    .bg{\r\n        position:fixed;\r\n        left:0;\r\n        top: 0;\r\n        width: 7.5rem;\r\n        height: auto;\r\n        z-index: -1;\r\n        // 设置滤镜模糊\r\n        filter: blur(150px);\r\n        // transform: scale(4);\r\n    }\r\n    .listViewTopNav{\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        height: 1.2rem;\r\n        font-size: 0.35rem;\r\n        .icon{\r\n            width: 0.5rem;\r\n            height: 0.5rem;\r\n            fill: #fff;\r\n        }\r\n        .title{\r\n            margin-left: 0.4rem;\r\n        }\r\n        .search{\r\n            margin-right: 0.5rem;\r\n        }\r\n    }\r\n    .left,.right{\r\n        display: flex;\r\n        color: #fff;\r\n    }\r\n\r\n\r\n    // 标题 头像部分\r\n    .content{\r\n        display: flex;\r\n        justify-content: space-between;\r\n        margin-top: 10px;\r\n    }\r\n    // 左边头像部分\r\n    .content_left{\r\n        position: relative;\r\n        // 头像\r\n        .hp{\r\n            position: absolute;\r\n            z-index: 2;\r\n            top: 8px;\r\n        }\r\n        .headPortrait{\r\n                width: 2.5rem;\r\n                height: 2.5rem;\r\n                border-radius: 10%;\r\n            }\r\n        .headPortraitBlock{\r\n            position: absolute;\r\n            z-index: 1;\r\n            left: 13px;\r\n            // 隐藏超出的部分\r\n            overflow: hidden;\r\n            border-radius: 10px;\r\n            .headPortraitBlockBG{\r\n                width: 2rem;\r\n                height: 2rem;\r\n                background-size: cover;\r\n                filter: blur(50px);\r\n                // 局部放大取颜色\r\n                transform: scale(3);\r\n            }\r\n        }\r\n         .count{\r\n            position: absolute;\r\n            right: 0.1rem;\r\n            top: 0.1rem;\r\n            font-size: 0.24rem;\r\n            color:rgb(246,247,250);\r\n            border-radius:10px;\r\n            background-color: rgba(19, 19, 19, 0.2);\r\n        }\r\n    }\r\n\r\n    // 右边歌单名和用户简介部分\r\n    .contnet_right{\r\n         width: 4rem;\r\n         height: 2.5rem;\r\n        .rightTop{\r\n            display: flex;\r\n            justify-content: space-around;\r\n            height: 1.25rem;\r\n            .UserTitle{\r\n                color: #fff;\r\n                font-size: 15px;\r\n            }\r\n            .icon{\r\n                fill: #fff;\r\n                width: 0.5rem;\r\n                height: 0.5rem;\r\n            }\r\n        }\r\n        // 用户信息\r\n        .UserInfo{\r\n            display: flex;\r\n            color: rgba(255, 255, 255, 0.5);\r\n            .UserPortrait{\r\n                width: 0.5rem;\r\n                height: 0.5rem;\r\n                border-radius: 100%;\r\n            }\r\n            .UserName{\r\n                height: 26px;\r\n                padding-left: 4px;\r\n                font-size: 10px;\r\n                line-height: 26px;\r\n            }\r\n            .UserAttention{\r\n                width: 0.7rem;\r\n                height: 0.5rem;\r\n                background-color: rgba(207, 207, 207, 0.5);\r\n                margin-left: 10px;\r\n                border-radius: 12px;\r\n                display: flex;\r\n                .icon{\r\n                    align-self: center;\r\n                    margin:0 auto;\r\n                    fill: rgba(255, 255, 255, 0.6);\r\n                }\r\n            }\r\n        }\r\n        // 用户简介\r\n        .UserAbout{\r\n                display: flex;\r\n                justify-content: space-around;\r\n                margin-top: 0.3rem;\r\n                width: 3.5rem;\r\n                color: rgba(255, 255, 255, 0.5);\r\n               .description{\r\n                    width: 3rem;\r\n                    font-size: 14px;\r\n                    display: -webkit-box;\r\n                    -webkit-line-clamp: 1;\r\n                    -webkit-box-orient: vertical;\r\n                    overflow: hidden;\r\n                }\r\n                .icon{\r\n                    align-self: center;\r\n                    margin:0 auto;\r\n                    fill: rgba(255, 255, 255, 0.6);\r\n                }\r\n        }\r\n    }\r\n\r\n}\r\n</style>\r\n"],"mappings":";;;EACSA,KAAK,EAAC;AAAa;;;;EA8BfA,KAAK,EAAC;AAAS;;EACXA,KAAK,EAAC;AAAc;;EAEhBA,KAAK,EAAC;AAAI;;;EAELA,KAAK,EAAC;AAAO;gEACfC,mBAAA,CAAc,cAAR,GAAC;;EAMVD,KAAK,EAAC;AAAmB;;;EAI7BA,KAAK,EAAC;AAAe;;EACjBA,KAAK,EAAC;AAAU;;EAEZA,KAAK,EAAC;AAAW;iEAItBC,mBAAA,CAIM;EAJDD,KAAK,EAAC;AAAoB,I,aAC3BC,mBAAA,CAEM;EAFDD,KAAK,EAAC,MAAM;EAAC,aAAW,EAAC;iBAC1BC,mBAAA,CAAsC;EAAjC,YAAU,EAAC;AAAe,G;;EAKtCD,KAAK,EAAC;AAAU;;EAEZA,KAAK,EAAC;AAAS;;;EAIfA,KAAK,EAAC;AAAU;iEAIrBC,mBAAA,CAIM;EAJDD,KAAK,EAAC;AAAe,I,aACtBC,mBAAA,CAEM;EAFDD,KAAK,EAAC,MAAM;EAAC,aAAW,EAAC;iBAC1BC,mBAAA,CAAmC;EAA9B,YAAU,EAAC;AAAY,G;;EAKnCD,KAAK,EAAC;AAAW;;EACbA,KAAK,EAAC;AAAa;iEAGxBC,mBAAA,CAIM;EAJDD,KAAK,EAAC;AAAe,I,aACtBC,mBAAA,CAEM;EAFDD,KAAK,EAAC,MAAM;EAAC,aAAW,EAAC;iBAC1BC,mBAAA,CAAuC;EAAlC,YAAU,EAAC;AAAgB,G;;;uBAnFxDC,mBAAA,CAyFM,OAzFNC,UAyFM,GAxFFF,mBAAA,CAAmD;IAA9CD,KAAK,EAAC,IAAI;IAAEI,GAAG,EAAEC,MAAA,CAAAC,QAAQ,CAACC,WAAW;IAAEC,GAAG,EAAC;uCAChDC,UA0BM,EACNC,mBAAA,aAAgB,EAChBT,mBAAA,CA0DM,OA1DNU,UA0DM,GAzDFV,mBAAA,CAcM,OAdNW,UAcM,GAbFF,mBAAA,QAAW,EACXT,mBAAA,CAOM,OAPNY,UAOM,GANFZ,mBAAA,CAA6D;IAAxDD,KAAK,EAAC,cAAc;IAAEI,GAAG,EAAEC,MAAA,CAAAC,QAAQ,CAACC,WAAW;IAAEC,GAAG,EAAC;uCACzDP,mBAAA,CAIK,OAJLa,UAIK,GAHFC,UAAc,EACdL,mBAAA,SAAY,EACZT,mBAAA,CAA8C,cAAAe,gBAAA,CAAtCC,MAAA,CAAAC,SAA