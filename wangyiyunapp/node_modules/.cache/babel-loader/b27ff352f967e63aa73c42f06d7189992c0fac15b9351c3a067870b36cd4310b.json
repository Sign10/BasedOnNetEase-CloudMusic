{"ast":null,"code":"//导入axios中封装的函数\nimport { getBanner } from '@/requests/api/home.js';\nimport { reactive, onMounted } from 'vue';\nexport default {\n  setup() {\n    const state = reactive({\n      images: []\n    });\n    onMounted(async () => {\n      let res = await getBanner();\n      state.images = res.data.banners;\n    });\n    return {\n      state\n    };\n  }\n};","map":{"version":3,"names":["getBanner","reactive","onMounted","setup","state","images","res","data","banners"],"sources":["D:\\桌面\\vue项目\\wangyiyunapp\\src\\components\\home\\SwpierTop.vue"],"sourcesContent":["<template>\r\n  <div id=\"swiperTop\">\r\n    <!-- 使用vant组件库的轮播图 -->\r\n    <van-swipe :autoplay=\"3000\" lazy-render>\r\n      <van-swipe-item v-for=\"image in state.images\" :key=\"image\">\r\n        <img :src=\"image.pic\" />\r\n      </van-swipe-item>\r\n    </van-swipe>\r\n  </div>\r\n</template>\r\n<script>\r\n//导入axios中封装的函数\r\nimport { getBanner } from '@/requests/api/home.js'\r\nimport { reactive, onMounted } from 'vue';\r\nexport default {\r\n  setup() {\r\n    const state = reactive({\r\n      images: []\r\n    });\r\n    onMounted(async () => {\r\n      let res = await getBanner();\r\n      state.images = res.data.banners\r\n    });\r\n    return { state };\r\n  },\r\n};\r\n</script>\r\n<style lang=\"less\">\r\n//以下是样式表\r\n#swiperTop {\r\n  margin: 2px;\r\n\r\n  .van-swipe {\r\n    width: 100%;\r\n    height: 3rem;\r\n\r\n    .van-swipe__track {\r\n      .van-swipe-item {\r\n        img {\r\n          width: 100%;\r\n          height: 100%;\r\n          border-radius: 0.2rem;\r\n        }\r\n      }\r\n    \r\n    .van-swipe__indicator--active {\r\n      background-color: rgb(219, 130, 130);\r\n    }\r\n  }\r\n}</style>"],"mappings":"AAWA;AACA,SAASA,SAAQ,QAAS,wBAAuB;AACjD,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACrBI,MAAM,EAAE;IACV,CAAC,CAAC;IACFH,SAAS,CAAC,YAAY;MACpB,IAAII,GAAE,GAAI,MAAMN,SAAS,EAAE;MAC3BI,KAAK,CAACC,MAAK,GAAIC,GAAG,CAACC,IAAI,CAACC,OAAM;IAChC,CAAC,CAAC;IACF,OAAO;MAAEJ;IAAM,CAAC;EAClB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}