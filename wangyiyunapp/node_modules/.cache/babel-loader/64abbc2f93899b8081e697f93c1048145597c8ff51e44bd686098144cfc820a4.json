{"ast":null,"code":"import { onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { getMusicItemList } from '@/requests/api/item';\nimport ItemMusicTop from '@/components/item/ItemMusicTop';\n//引入子组件\n//从这里我导入的是axios中的服务，返回getMusicItemList这个函数\n//获取路由的参数或者是数据\n\nexport default {\n  setup() {\n    const state = reactive({\n      playlist: {}\n    });\n    onMounted(async () => {\n      //获取到里面的id属性,id为query里的一个属性\n      let id = useRoute().query.id;\n      let res = await getMusicItemList(id);\n      state.playlist = res.data.playlist;\n    });\n    return {\n      state\n    };\n  },\n  components: {\n    ItemMusicTop\n  }\n};","map":{"version":3,"names":["onMounted","useRoute","getMusicItemList","ItemMusicTop","setup","state","reactive","playlist","id","query","res","data","components"],"sources":["D:\\桌面\\vue项目\\wangyiyunapp\\src\\views\\ItemMusic.vue"],"sourcesContent":["<template>\r\n    <div>歌单详情页</div>\r\n    <ItemMusicTop :playlist=\"state.playlist\r\n    \"/>\r\n</template>\r\n\r\n<script>\r\nimport { onMounted } from 'vue';\r\nimport { useRoute } from 'vue-router';\r\nimport { getMusicItemList } from '@/requests/api/item';\r\nimport ItemMusicTop from '@/components/item/ItemMusicTop'\r\n//引入子组件\r\n//从这里我导入的是axios中的服务，返回getMusicItemList这个函数\r\n//获取路由的参数或者是数据\r\n\r\nexport default {\r\n    setup() {\r\n\r\n\r\n        const state = reactive({\r\n            playlist: {}\r\n        })\r\n\r\n\r\n        onMounted(async () => {\r\n            //获取到里面的id属性,id为query里的一个属性\r\n            let id = useRoute().query.id\r\n            let res = await getMusicItemList(id);\r\n            state.playlist = res.data.playlist\r\n        })\r\n        return { state }\r\n    },\r\n\r\n    components:{\r\n        ItemMusicTop,\r\n    }\r\n}\r\n\r\n\r\n\r\n</script>"],"mappings":"AAOA,SAASA,SAAQ,QAAS,KAAK;AAC/B,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,gBAAe,QAAS,qBAAqB;AACtD,OAAOC,YAAW,MAAO,gCAA+B;AACxD;AACA;AACA;;AAEA,eAAe;EACXC,KAAKA,CAAA,EAAG;IAGJ,MAAMC,KAAI,GAAI