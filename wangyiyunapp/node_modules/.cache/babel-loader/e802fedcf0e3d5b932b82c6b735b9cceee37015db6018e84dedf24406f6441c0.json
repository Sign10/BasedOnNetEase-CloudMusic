{"ast":null,"code":"import { Vue3Marquee } from 'vue3-marquee';\nimport 'vue3-marquee/dist/style.css';\nimport { mapMutations, mapState } from 'vuex';\nexport default {\n  data() {\n    return {\n      isLyricShow: false\n    };\n  },\n  computed: {\n    ...mapState(['lyricList']),\n    getLyric() {\n      if (this.lyricList.lyric) {\n        let arr;\n        arr == this.lyricList.lyric.split(/[(\\r\\n)\\r\\n]+/).map((item, i) => {\n          let min = item.match(/\\d{2}(?=:)/gm);\n          let sec = item.match(/d{2}(?=\\.)/gm);\n          let millSec = item.match(/\\d{2,3}(?=])/gm);\n          let lyric = item.match(/([^\\]]+)$/gm);\n        });\n      }\n    }\n  },\n  mounted() {\n    console.log(this.lyricList.lyric, '这个也是歌詞');\n  },\n  components: {\n    Vue3Marquee\n  },\n  methods: {\n    ...mapMutations(['updatesonglistmore'])\n  },\n  props: ['musiclist', 'play', 'isbtnShow']\n};","map":{"version":3,"names":["Vue3Marquee","mapMutations","mapState","data","isLyricShow","computed","getLyric","lyricList","lyric","arr","split","map","item","i","min","match","sec","millSec","mounted","console","log","components","methods","props"],"sources":["D:\\桌面\\vue项目\\wangyiyunapp\\src\\components\\item\\MusicDetail.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <img :src=\"musiclist.al.picUrl\" alt=\"\" class=\"bgimg\">\r\n        <div class=\"detailTop\">\r\n            <div class=\"topleft\">\r\n                <svg class=\"icon3\" aria-hidden=\"true\" @click=\"updatesonglistmore\">\r\n                    <use xlink:href=\"#icon-zuojiantou\"></use>\r\n                </svg>\r\n                <div class=\"leftMarquee\">\r\n                    <Vue3Marquee class=\"marquee\">{{ musiclist.al.name }}</Vue3Marquee>\r\n                    <div class=\"sonMarquee\">\r\n                        <span v-for='(item, idx) in musiclist.ar' :key=\"item\" class=\"arname\">\r\n                            {{ item.name }}\r\n                            {{ idx === musiclist.ar.length - 1 ? \"\" : \"/\" }}\r\n                        </span>\r\n                        <svg class=\"icon2\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-youjiantou1\"></use>\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n\r\n            </div>\r\n            <div class=\"topright\">\r\n                <svg class=\"icon1\" aria-hidden=\"true\">\r\n                    <use xlink:href=\"#icon-fenxiang\"></use>\r\n                </svg>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"detailContent\"  v-show=\"isLyricShow\">\r\n            <img src=\"@/assets/needle-ab.png\" alt=\"\" class=\"img_needle\" :class=\"{ img_needle_active: !isbtnShow }\">\r\n            <img src=\"@/assets/cd.png\" alt=\"\" class=\"img_cd\">\r\n            <img :src=\"musiclist.al.picUrl\" alt=\"\" class=\"img_ar\"\r\n                :class=\"{ img_ar_active: !isbtnShow, img_ar_pause: isbtnShow }\">\r\n        </div>\r\n\r\n        <div class=\"Musiclyric\">\r\n            {{ lyricList.lyric }}\r\n        </div>\r\n\r\n        <div class=\"detailFooter\">\r\n            <div class=\"footerTop\">\r\n                <svg class=\"icon5\" aria-hidden=\"true\">\r\n                    <use xlink:href=\"#icon-aixin\"></use>\r\n                </svg>\r\n                <svg class=\"icon5\" aria-hidden=\"true\">\r\n                    <use xlink:href=\"#icon-iconfontzhizuobiaozhun023146\"></use>\r\n                </svg>\r\n                <svg class=\"icon5\" aria-hidden=\"true\">\r\n                    <use xlink:href=\"#icon-yinlechangpian\"></use>\r\n                </svg>\r\n                <svg class=\"icon5\" aria-hidden=\"true\">\r\n                    <use xlink:href=\"#icon-iconfontzhizuobiaozhun023110\"></use>\r\n                </svg>\r\n                <svg class=\"icon5\" aria-hidden=\"true\">\r\n                    <use xlink:href=\"#icon-liebiao-\"></use>\r\n                </svg>\r